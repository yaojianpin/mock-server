{

  "config": {

    "routing": {
      "/my_image/:id": {
        "to": "/file/:id"
      },
      "/a/b/c/:id": { 
        "to": "/api/data/:id"
      },
      "/a/b/data2": { 
        "to": "/api/data2"
      },
      "/c/data3": { 
        "to": "/api/data3"
      },
      "/api/v1/friends" : {
        "to": "/api/friends",
        "query": {
          "name": "_q",
          "p": "_page",
          "s": "_size"
        }
      },
      "/api/v1/friend/:id" : {
        "to": "/api/friends/:id",
        "rules": [
          { "key": "id", "match": "\\d+",  "message": "id must be number" }
        ],
        "wrapping": {
          "ok": {
            "status": "success",
            "nested": {
              "data": "$data"
            }
          },
          "err": {
            "status": "error",
            "my_message": "$msg",
            "others": "others"
          }
        }

      }
    },

    "wrapping": {

      "ok": {
        "code": 200,
        "msg": "$msg", 
        "data": "$data"
      },
      "err": {
        "code": 500,
        "msg": "$msg",
        "data": "$data"
      },
      "pagination": {
        "page": "$page",
        "size": "$size",
        "total": "$total",
        "rows": "$items"
      }

    }
  },
  "data": {
      "data1|100": [
      {
        "id|+1": 1,
        "i2": "@uuid",
        "ser|+1": [ "a", "bb", "c" ],
        "start|3": "a",
        "pick|1": ["a", "b", "c"],
        "float|.1-10": 0.5,
        "float2|10-100.3-10": 100.5,
        "name": "@name",
        "word": "@word(5)",
        "word2": "@word(1, 10)",
        "sentence": "@sentence",
        "sentence2": "@sentence(1, 10)",
        "paragraph": "@paragraph",
        "paragraph2": "@paragraph(1, 10)"
      }
    ],

    "data2" : {
      "id": "@uuid"
    },

    "data3": "@name",
    "friends|20": [{
      "name": "@name",
      "age|1-100": 20,
      "id|+1": 1
    }]
  },

  "file": {
    "image1": "./static/image1.jpg",
    "text1": "./static/text1.txt",
    "json1": "./static/json1.json"
  }
}